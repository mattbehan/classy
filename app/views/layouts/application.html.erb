<!DOCTYPE html>
<html>
<head>
  <title>TeacherGroups</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= form_tag 'teachers/upload', :multipart => true do %>
   <label for="file">Upload csv File</label> <%= file_field_tag "file" %>
   <%= submit_tag "Save changes", id: "save" %>
<% end %>

<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>

<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>

<%= yield %>

</body>
</html>
